{
  "presets": [
    ["@babel/preset-env", { "modules": false, "useBuiltIns": "usage", "debug": false }],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "overrides": [
    {
      "test": ["./config", "./tasks", "gulpfile.babel.js"],
      "presets": [["@babel/preset-env"]]
    }
  ]
}
